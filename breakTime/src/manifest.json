{
  "name": "breakTime",
  "version": "1.0",
  "description": "Build an extension that is like a visual alarm",
  "manifest_version": 2,
  "permissions": ["storage", "alarms", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "switch.html"
  }
}